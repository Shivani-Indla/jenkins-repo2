pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo Hi this is my Build'
            }
        }
        stage('Test') {
            steps {
                sh 'echo Hi this is my Test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo Hi this is my Deploy'
            }
        }
    }
}